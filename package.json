{
  "name": "chaos-equations",
  "version": "0.0.0",
  "description": "Animations of 2-D parametric equations that move points recursively.",
  "author": "Andrew Carlson <acarl005@g.ucla.edu>",
  "scripts": {
    "start": "node --experimental-modules server.mjs",
    "build": "rm -rf dist && parcel build --public-url /chaos-equations-js src/index.html",
    "publish": "npm run build && node --experimental-modules publish.mjs",
    "lint": "eslint src/ --ext js --ext jsx",
    "dev": "parcel src/index.html"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reset-css": "^4.0.1",
    "stats-js": "^1.0.0",
    "three": "^0.103.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "repository": {
    "url": "https://github.com/acarl005/chaos-equations-js"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "chaos",
    "equations",
    "webgl",
    "animations",
    "visualizations",
    "three.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3"
  }
}
